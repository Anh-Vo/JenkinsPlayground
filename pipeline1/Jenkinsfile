pipeline {
  agent any
  options{
    parameters([booleanParam(deaultValue: true, description: '', name: 'build'),
       booleanParam(defaultValue: true, description: '', name: 'deploy')])
 }
  
  stages{
    stage ('Print Hello') {
      steps {
        echo 'Hello' 
      }  
    }

    stage('Print World') {
      steps {
        echo 'World!'
      } 
    }
  }
   
}
